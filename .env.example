# AWS Configuration
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# API Gateway
NEXT_PUBLIC_API_URL=https://your-api-gateway-url.amazonaws.com/prod

# Cognito Configuration
NEXT_PUBLIC_COGNITO_USER_POOL_ID=your_user_pool_id
NEXT_PUBLIC_COGNITO_CLIENT_ID=your_client_id

# DynamoDB Tables
DYNAMODB_CERTIFICATES_TABLE=certificates
DYNAMODB_CURRICULUMS_TABLE=curriculums
DYNAMODB_USERS_TABLE=users

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_KNOWLEDGE_BASE_ID=your_knowledge_base_id

# Q-Net API (Korean Certification API)
QNET_SERVICE_KEY=your_qnet_service_key
QNET_BASE=http://openapi.q-net.or.kr/api/service/rest
